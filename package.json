{
    "name": "webapp-vue-template",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "npm run server:dev",
        "prod": "npm run build && npm run server:prod",
        "build": "vite build",
        "build:check": "run-p type-check build",
        "server": "node --loader ts-node/esm ./server/index.ts",
        "server:dev": "npm run server",
        "server:prod": "cross-env NODE_ENV=production npm run server",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@tsconfig/node20": "^20.1.4",
        "@types/compression": "^1.7.5",
        "@types/express": "^5.0.0",
        "@types/node": "^20.16.5",
        "@vitejs/plugin-vue": "^5.1.3",
        "@vitejs/plugin-vue-jsx": "^4.0.1",
        "@vue/compiler-sfc": "^3.5.8",
        "@vue/server-renderer": "^3.5.8",
        "@vue/tsconfig": "^0.5.1",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "express": "^4.21.0",
        "node-fetch": "^3.3.2",
        "pinia": "^2.2.2",
        "sirv": "^2.0.4",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.5",
        "vike": "^0.4.196",
        "vite": "^5.4.4",
        "vue": "^3.5.4",
        "vue-router": "^4.4.4",
        "vue-tsc": "^2.1.6"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^3.5.1",
        "@unocss/eslint-plugin": "^0.62.3",
        "eslint": "^9.10.0",
        "eslint-plugin-format": "^0.1.2",
        "npm-run-all2": "^6.2.2"
    }
}
